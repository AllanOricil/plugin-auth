      "client-id",
      "disable-masking",
      "instance-url",
      "json",
      "loglevel",
      "no-prompt",
      "set-default",
      "set-default-dev-hub"
    ],
    "alias": ["force:auth:web:login", "auth:web:login"]
  },
  {
    "command": "org:logout",
    "plugin": "@salesforce/plugin-auth",
    "flags": ["all", "json", "loglevel", "no-prompt", "target-org"],
