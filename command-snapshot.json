    "plugin": "@salesforce/plugin-auth",
    "flags": [
      "alias",
      "client-id",
      "disable-masking",
      "instance-url",
      "json",
      "loglevel",
      "set-default",
      "set-default-dev-hub"
    ],
    "alias": ["force:auth:device:login", "auth:device:login"]
  },
  {
    "command": "org:login:jwt",
    "plugin": "@salesforce/plugin-auth",
    "flags": [
      "alias",
      "client-id",
      "instance-url",
      "json",
      "jwt-key-file",
      "loglevel",
      "no-prompt",
      "set-default",
      "set-default-dev-hub",
